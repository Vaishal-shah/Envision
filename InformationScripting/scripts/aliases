classes = ast -t Class
methods = ast -t Method
toClass = toParent -t Class
astFilter = ast -input
addTags = tags -a
removeTage = tags -r
info = show -info

cs-ins = ast -t Class -global | case-studies/instability | toHtml
cs-why = ast -t Statement -tl | changes -c 2 | join -v change.id,commit.message,ast -as astMessage| case-studies/associatedBugs
cs-args = callgraph -global | canReach -self -r calls | changes -nodes | case-studies/printArgs
