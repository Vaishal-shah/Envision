classes = ast -t Class
methods = ast -t Method
toClass = toParent -t Class
astFilter = ast -input
addTags = tags -a
removeTage = tags -r
info = show -info

cs-if-with-else = ast -global -t Statement | case-studies/if-with-else
cs-csv = profileSource scripts/carmodel.csv | heatmap
cs-ins = ast -t Class -global | case-studies/instability | table
cs-why = ast -t Statement -tl|changes -c 2 -intermediate|join -v change.id,commit.message,ast -as data|case-studies/associatedBugs
cs-args = callgraph -global | reachable -self | changes -nodes | case-studies/printArgs
